{
  "name": "aprende-angular-2-facilmente",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "merge-reports": "mochawesome-merge ./cypress/report/*.json | out-file --encoding ascii ./cypress/report/all-reports.json",
    "generate-htmlreport": "marge ./cypress/report/%npm_config_fileName%.json --reportDir ./cypress/report/html --inline",
    "reports": "cypress run --reporter mochawesome"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.5",
    "@popperjs/core": "^2.9.3",
    "angular2-in-memory-web-api": "0.0.15",
    "bootstrap": "^4.5.2",
    "bootswatch": "^4.5.2",
    "core-js": "^2.4.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "cypress": "^9.5.1",
    "cypress-file-upload": "^5.0.8",
    "lite-server": "^2.2.0",
    "mochawesome": "^7.1.2",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.1.1",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
